{
    "name": "asset-transfer-basic-cass",
    "version": "1.0.0",
    "description": "Asset Transfer Basic-cass Application implemented in typeScript using fabric-gateway",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "transfer": "tsc && node dist/transferFabric.js",
        "build": "tsc",
        "build:watch": "tsc -w",
        "lint": "eslint . --ext .ts",
        "prepare": "npm run build",
        "pretest": "npm run lint",
        "start": "tsc && node dist/app.js"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "@hyperledger/fabric-gateway": "^1.1.1",
        "dotenv": "^16.1.4",
        "ethers": "^6.4.1"
    },
    "devDependencies": {
        "@tsconfig/node14": "^1.0.3",
        "@types/node": "^14.18.16",
        "@typescript-eslint/eslint-plugin": "^5.22.0",
        "@typescript-eslint/parser": "^5.22.0",
        "eslint": "^8.14.0",
        "typescript": "~4.6.4"
    }
}
